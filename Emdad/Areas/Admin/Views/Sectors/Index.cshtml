@model IEnumerable<Emdad.Models.Sectors>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SectorsName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SectorsLink)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SectorIcon)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SectorDesc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsDelete)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EditId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EditDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SectorsName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SectorsLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SectorIcon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SectorDesc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsDelete)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EditId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EditDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SectorsId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SectorsId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SectorsId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
